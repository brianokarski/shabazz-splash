html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video
  border 0
  font-size 100%
  font inherit
  vertical-align baseline
  margin 0
  padding 0

article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section
  display block

body
  line-height 1
  font-family Helvetica, sans-serif
  font-size 20px
  line-height 28px
  letter-spacing .02em
  font-family Helvetica, sans-serif
  font-size 20px
  line-height 28px
  letter-spacing .02em

ol,ul
  list-style none

blockquote,q
  quotes none

blockquote:before,blockquote:after,q:before,q:after
  content none

table
  border-collapse collapse
  border-spacing 0

*

html
  background-color #ff0918

.mobile-pub
  display none
  margin-top 28px
  display block
  float left
  margin-left 5px
  width 310px

#sp-logo
  img
    width 80px
    float left
    fill inherit

#main-ul
  padding-top 10px
  padding-top 3px
  & > li
    &:hover
      & > .link-title
        &:after
          opacity 0
  & > .pub-li
    &:hover
      & > .link-title
        &:after
          margin-left 20px
          opacity 1
          -webkit-transform rotate(45deg)
          -webkit-transform-origin 0px 0px
          -webkit-transition all .25s
          -webkit-transform translateZ(1) scale(1.0, 1.0)

li
  display block
  float left
  margin-right 10px
  position relative
  padding-right 0.5em
  line-height 27px

#sp-ul, #pub-ul
  display block
  -webkit-transition opacity .25s ease-out
  opacity 0
  height 0
  overflow hidden
  position absolute
  left 0

#main-ul > li:hover > #sp-ul,
#main-ul > li:hover > #pub-ul
  opacity 1
  height auto
  overflow visible
  -webkit-transition opacity .25s ease-out

a
  color inherit
  text-decoration none
  &.slide-left
    z-index 200
    position absolute
    top 45%
    left 4px
    -webkit-transform rotate(90deg)
  &.slide-right
    z-index 200
    position absolute
    top 45%
    right 4px
    -webkit-transform rotate(-90deg)

.sp-li
  margin-left 15px
  margin-left 5px

.shabazz-intro
  position absolute
  top 0px
  left 0px
  width 460px
  position static
  float left
  width 310px
  a
    &:hover
      color white
      border-bottom 2px solid white

#sp-ul
  width 500px

.pub-li
  text-align left
  float right
  margin-right 220px

#pub-ul
  width 400px
  line-height 27px
  li
    padding-top 27px
    line-height 27px
    a
      display block

.cart
  position absolute
  top 10px
  right 15px

.cartbtn, .addcartbtn, .minicart-submit
  font-family Helvetica
  font-size 20px
  line-height 28px
  letter-spacing .02em
  background none
  border none
  color black
  text-decoration none
  cursor pointer
  box-shadow 0px
  padding 0px
  margin 0px
  border 0px

.cartbtn:focus, .addcartbtn:focus, button:focus
  box-shadow 0px
  padding 0px
  margin 0px
  border 0px
  outline none

form
  &.addtocart
    display inline
    margin-left 10px
  &.carttwo
    position absolute
    bottom 15px
    left 15px

#info
  position absolute
  bottom 10px
  left 15px

.sp-intro
  margin-top -28px

.sp-intro a, .udln, form.addtocart, .shabazz-intro a, .minicart-submit
  border-bottom 2px solid black

p
  margin-bottom 28px
  &.first
    text-indent 165px

.link-title
  position relative
  z-index 100
  &:after
    position absolute
    margin-left 9px
    float right
    content "+"
    width 25px
    height 15px
    opacity 1
    -webkit-transform rotate(0deg)
    -webkit-transform-origin 25% 0%
    -webkit-transition all .25s
    -webkit-backface-visibility hidden
    -webkit-transform translateZ(1) scale(1.0, 1.0)

.booktitle
  display block

#img-container
  img
    position absolute
    max-width 85%
    max-height 85%
    top 10%
    left 10%
    border-radius 1px
    z-index -400
    &:empty
      top 50%
      left 50%
      -webkit-transform translate(-50%, -50%)
      -moz-transform translate(-50%, -50%)
      -ms-transform translate(-50%, -50%)
      -o-transform translate(-50%, -50%)
      transform translate(-50%, -50%)
      -webkit-transform translateZ(1) scale(1.0, 1.0)

.imgdisplay
  display visible !important

#library
  &:hover
    color red

#hawlin
  &:hover
    color green

#jazz
  &:hover
    color red

#sega
  &:hover
    color pink

#navsegda
  &:hover
    color maroon

#ball
  &:hover
    color brown

#library-img,
#hawlin-img,
#jazz-img,
#sega-img,
#navsegda-img,
#ball-img
  display none

.bglibrary
  color white !important
  background-color black

.bghawlin
  background-color yellow

.bgjazz
  background-color white

.bgsega
  background-color pink

.bgnavsegda
  background-color grey

.bgball
  background-color beige

.bgfadelibrary
  color white !important
  background-color black
  transition all .15s ease-out

.bgfadehawlin
  background-color yellow
  transition all .15s ease-out

.bgfadejazz
  background-color white
  transition all .15s ease-out

.bgfadesega
  background-color pink
  transition all .15s ease-out

.bgfadenavsegda
  background-color grey
  transition all .15s ease-out

.bgfadeball
  background-color beige
  transition all .15s ease-out

#hawlin-gallery,
#jazz-gallery,
#sega-gallery,
#navsegda-gallery,
#ball-gallery
  position absolute
  overflow hidden
  display none
  z-index 150
  width 100%
  height 100%
  top 0
  left 0

.box
  img
    z-index 200
    position absolute
    max-width 85%
    max-height 85%
    display none
    cursor pointer !important
    &:empty
      top 50%
      left 50%
      -webkit-transform translate(-50%, -50%)
      -moz-transform translate(-50%, -50%)
      -ms-transform translate(-50%, -50%)
      -o-transform translate(-50%, -50%)
      transform translate(-50%, -50%)
      -webkit-transform translateZ(1) scale(1.0, 1.0)

.slide-title
  position absolute
  top 10px
  left 15px

.imgslide
  display none

.preload
  cursor wait !important
  display none
  position absolute
  z-index 250
  top 50%
  left 50%
  -webkit-transform translate(-50%, -50%)
  -moz-transform translate(-50%, -50%)
  -ms-transform translate(-50%, -50%)
  -o-transform translate(-50%, -50%)
  transform translate(-50%, -50%)

#intro-page
  position absolute
  top 0
  left 0
  width 100%
  height 100%
  background black
  z-index 500
  svg
    fill red

#logo
  height 100%
  width 100%
  top 50%

input
  border 0px
  margin-left 0px
  padding 0px

.email-fm
  margin 0px
  padding 0px

.email
  font-family Helvetica, sans-serif
  display inline
  width 176px
  height 25px
  background #000
  color #000 !important
  text-align left
  padding 0px
  margin-right 10px
  font-size 20px
  line-height 28px
  letter-spacing .02em
  outline none
  background-color transparent
  cursor text
  border-bottom 2px solid black
  -webkit-transition width .25s ease-in-out
  -moz-transition width .25s ease-in-out
  -o-transition width .25s ease-in-out
  transition width .25s ease-in-out
  &:focus
    width 220px
    & + .submitbtn
      display inline
      font-family Helvetica
      font-size 20px
      letter-spacing .02em
      border none
      text-decoration none
      cursor pointer
      box-shadow 0px
      padding 0px 1px
      margin 0px
      border 0px
      color black
  & + .submitbtn
    display none
    -webkit-transition width .25s ease-in-out
    -moz-transition width .25s ease-in-out
    -o-transition width .25s ease-in-out
    transition width .25s ease-in-out

.submitbtn
  font-family Helvetica
  font-size 21px
  line-height 28px
  letter-spacing .02em
  background none
  border 2px solid black
  color black
  text-decoration none
  cursor pointer
  box-shadow 0px
  padding 0px
  margin 0px
  border 0px

::-webkit-input-placeholder
  color #000

:-moz-placeholder
  color #000

::-moz-placeholder
  color #000

:-ms-input-placeholder
  color #000

#PPMiniCart
  display none
  position fixed
  top 50%
  left 50%
  -webkit-transform translate(-50%, -50%)
  -moz-transform translate(-50%, -50%)
  -ms-transform translate(-50%, -50%)
  -o-transform translate(-50%, -50%)
  transform translate(-50%, -50%)
  z-index 350
  .minicart-title
    float left
    margin -10px -10px 0
    padding 10px
    background 0
    border 0
    font-size 20px
    cursor pointer
    position absolute
    top 12px
  form
    position relative
    width 600px
    max-height 400px
    margin-left 0px
    padding 10px 10px 40px
    background #fff
    border 0px
    border-radius 0px
    box-shadow 0px 0px 0px rgba(0, 0, 0, 0)
    font 20px/normal helvetica
    color #000
    &.minicart-empty
      padding-bottom 10px
      font-size 20px
  ul
    clear left
    margin 50px 0px 21px 0px
    list-style-type none
    background #fff
    border 0px
    border-radius 0px
    box-shadow 0px 0px 0px rgba(0, 0, 0, 0)
  .minicart-empty
    ul
      display none
  .minicart-closer
    float right
    margin -12px -10px 0
    padding 10px
    background 0
    border 0
    font-size 20px
    cursor pointer
  .minicart-item
    position relative
    padding 3px 0
    min-height 17px
    float none
    font-size 20px
    line-height 7px
    & + .minicart-item
    a
      position absolute
      top 11px
      left 0
      width 350px
      color #333
      line-height 12px
      text-decoration none
  .minicart-attributes
    margin 18px 0 0
    padding 0
    border 0
    border-radius 0
    box-shadow none
    color #999
    font-size 12px
    line-height 22px
    li
      display inline
      &:after
        content ","
      &:last-child
        &:after
          content ""
  .minicart-quantity
    position absolute
    top -4px
    left 430px
    width 24px
    height 21px
    padding 0px 4px
    font-size 19px
    text-align right
    transition border linear 0.2s, box-shadow linear 0.2s
    -webkit-transition border linear 0.2s, box-shadow linear 0.2s
    -moz-transition border linear 0.2s, box-shadow linear 0.2s
    &:hover
      border-color #0078C1
    &:focus
      border-color #0078C1
      outline 0
      box-shadow inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 3px rgba(0, 120, 193, 0.4)
  .minicart-remove
    position absolute
    top -7px
    left 462px
    width 18px
    height 23px
    margin 0
    padding 0
    background #fff
    border 1px solid #fff
    color red
    font-size 20px
    opacity 1
    cursor pointer
    &:hover
      opacity .5
  .minicart-price
    position absolute
    top 3px
    right 0
  .minicart-subtotal
    position absolute
    bottom 17px
    left 10px
    font-size 20px
    color red
  .minicart-submit
    position absolute
    bottom 12px
    right 10px
    cursor pointer
    img
      vertical-align middle
      padding 4px 0 0 2px

.minicart-showing
  #PPMiniCart
    display block
    top 50%
    left 50%
    -webkit-backface-visibility hidden
    -webkit-transform translate(-50%, -50%)
    -moz-transform translate(-50%, -50%)
    -ms-transform translate(-50%, -50%)
    -o-transform translate(-50%, -50%)
    transform translate(-50%, -50%)

.minicart-empty-text
  margin-top 40px
  margin-bottom 5px

@media screen and (max-width: 880px)
    html
        background-color #e6000e


@media screen and (max-width: 768px)
    html
        background-color #c4000c

@media only screen and (min-device-width: 481px) and (max-device-width: 1024px) and (orientation:portrait)
    html
        background-color #800008

@media only screen and (min-device-width: 481px) and (max-device-width: 1024px) and (orientation:landscape)
    html
        background-color #5e0006

@media only screen and (max-width : 320px)
    html
        background-color #ff0918

    .pub-li, .cart
      display none

    .mob-blade
      margin 28px 0px
      text-align center

    .slide-mob
      img
        width 100%